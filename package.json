    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "got": "^11.7.0",
    "nodemon": "^2.0.4",
    "xo": "^0.28.0"
  },
  "optionalDependencies": {
    "sqlite3": "^5.0.0"
